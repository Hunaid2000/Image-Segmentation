<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>


<AnchorPane prefHeight="400.0" prefWidth="400.0" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/17" fx:controller="application.AppController">
   <children>
      <AnchorPane prefHeight="400.0" prefWidth="400.0">
         <children>
            <TextField fx:id="Clusters" layoutX="125.0" layoutY="37.0" promptText="Enter Clusters" />
            <TextField fx:id="MaxIter" layoutX="125.0" layoutY="76.0" promptText="Enter Max Iterations" />
            <Button fx:id="FileChooseButton" layoutX="23.0" layoutY="130.0" mnemonicParsing="false" onAction="#ChooseFileAction" prefHeight="30.0" prefWidth="91.0" text="Choose File" />
            <Label fx:id="FileName" layoutX="125.0" layoutY="132.0" prefHeight="26.0" prefWidth="250.0" />
            <Button fx:id="StartButton" layoutX="155.0" layoutY="185.0" mnemonicParsing="false" onAction="#StartAction" prefHeight="30.0" prefWidth="91.0" text="Start" />
         </children>
      </AnchorPane>
   </children>
</AnchorPane>
